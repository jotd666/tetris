	.include	"tetris.inc"
	
	.global		tetris_reset
	.global		tetris_irq
	
	.macro	WPSET	addr
	clr.b	\addr
	.endm
	
	.macro	LOG_PC	addr
	jbsr		1234567f
1234567:
	move.l	(a7)+,\addr
	.endm
	
	.macro	SET_X_FROM_CLEARED_C
	PUSH_SR
	move.w	(sp),d4
	bset	#4,d4   | set X
	btst	#0,d4
	beq.b	0f
	bclr	#4,d4   | C is set: clear X
0:
	move.w	d4,(sp)
	POP_SR
	.endm

	* useful to recall C from X (add then move then bcx)
	.macro	SET_C_FROM_X
	PUSH_SR
	move.w	(sp),d4
	bset	#0,d4   | set C
	btst	#4,d4
	bne.b	0f
	bclr	#0,d4   | X is clear: clear C
0:
	move.w	d4,(sp)
	POP_SR
	.endm

	
	.macro INVERT_XC_FLAGS
	PUSH_SR
	move.w	(sp),d4
	eor.b	#0x11,d4
	move.w	d4,(sp)
	POP_SR
	.endm
	
	.macro CLR_XC_FLAGS
	moveq	#0,d7
	roxl.b	#1,d7
	.endm
	.macro SET_XC_FLAGS
	st	d7
	roxl.b	#1,d7
	.endm
	
	.macro	SBC_X	address
	INVERT_XC_FLAGS
	GET_ADDRESS	\address
	move.b	(a0,d1.w),d4
	subx.b	d4,d0
	INVERT_XC_FLAGS
	.endm
	
	.macro	SBC_Y	address
	INVERT_XC_FLAGS
	GET_ADDRESS	\address
	move.b	(a0,d2.w),d4
	subx.b	d4,d0
	INVERT_XC_FLAGS
	.endm
	
	.macro	SBC	address
	INVERT_XC_FLAGS
	GET_ADDRESS	\address
	move.b	(a0),d4
	subx.b	d4,d0
	INVERT_XC_FLAGS
	.endm
	
	.macro	SBC_IMM	param
	INVERT_XC_FLAGS
	move.b	#\param,d4
	subx.b	d4,d0
	INVERT_XC_FLAGS
	.endm
	
	.macro CLR_V_FLAG
	moveq	#0,d3
	add.b	d3,d3
	.endm

	.macro SET_I_FLAG
	jbsr	osd_disable_interrupts
	.endm
	.macro CLR_I_FLAG
	jbsr	osd_enable_interrupts
	.endm
	.ifdef	MC68020
	.macro PUSH_SR
	move.w	ccr,-(sp)
*	ror.w	#8,d2
*	tst.b	d2
*	beq.b	123456f
*	jbsr	osd_break
*	nop
*123456:
*	rol.w	#8,d2
*	move.w	(sp),ccr
	.endm
	.macro POP_SR
	move.w	(sp)+,ccr
	.endm
	.else
	.macro PUSH_SR
	move.w	sr,-(sp)
	.endm
	.macro POP_SR
	move.w	(sp)+,sr
	.endm
	.endif

	.macro READ_LE_WORD	srcreg
	PUSH_SR
	moveq	#0,d4
	move.b	(1,\srcreg),d4
	lsl.w	#8,d4
	move.b	(\srcreg),d4
	* we have to use long else it will
	* extend sign for > 0x7FFF and will compute wrong offset
	move.l	d4,\srcreg
	POP_SR
	.endm

	.macro	BIT	arg
	move.b	d0,d4
	and.b	\arg,d4
	.endm

	.macro GET_ADDRESS	offset
	lea	\offset,a0
	jbsr	get_address
	.endm

	.macro GET_ADDRESS_X	offset
	lea	\offset,a0
	jbsr	get_address
	lea	(a0,d1.w),a0
	READ_LE_WORD	a0
	jbsr	get_address
	.endm

	.macro GET_ADDRESS_Y	offset
	GET_ADDRESS	\offset
	READ_LE_WORD	a0
	jbsr	get_address
	.endm

	* doesn't check anything, but osd_w_colorram will
	.macro GET_SCREEN_ADDRESS_Y	offset
	lea	(a5,\offset),a0
	READ_LE_WORD	a0
	add.w	d2,a0
	.endm
	
	.macro GET_UNCHECKED_ADDRESS_Y	offset
	lea	(a5,\offset),a0
	READ_LE_WORD	a0
	.endm
	
	* doesn't check anything, but osd_set_palette_entry will
	.macro GET_PALETTE_ADDRESS_Y	offset
	lea	(a5,\offset),a0
	READ_LE_WORD	a0
	* read some address between 0x2000 and 0x2100, center on 0-0x100
	sub.w	#0x2000,a0
	add.w	d2,a0
	.endm

	.macro	STORE_D0_TO_PALETTE	address
	lea		\address-0x2000,a0
	jbsr	osd_set_palette_entry
	.endm
	
cpu_init:
	moveq	#0,d0
	moveq	#0,d1
	moveq	#0,d2
	lea		rom_8000-0X8000,a6
	lea		game_ram,a5
	rts

* < A0: offset
* flags preserved
get_address:
	PUSH_SR
	cmp.w	#0x1000,a0
	bcc.b	0f
	* RAM
	add.l	a5,a0
	POP_SR
	rts
0:
	* above 0x1000
	cmp.w	#0x2100,a0
	bcc.b	0f
	lea		address_1000_20FF_loaded_message(pc),a1
	jbsr	osd_break
	POP_SR
	illegal
0:
	* above 0x2100
	cmp.w	#0x2400,a0
	bcc.b	0f
	lea		address_2100_23FF_loaded_message(pc),a1
	jbsr	osd_break
	POP_SR
	illegal
0:
	* above 0x2400
	cmp.w	#0x2600,a0
	bcc.b	0f
	* EEPROM
	lea		eeprom_2400-0x2400,a0
	POP_SR
	rts
0:
	* above 0x2600
	cmp.w	#0x4000,a0
	bcc.b	0f
	lea		address_2600_3FFF_loaded_message(pc),a1
	jbsr	osd_break
	POP_SR
	illegal	
	illegal	
0:
	* above 0x4000: caution: compare with long!!
	cmp.l	#0x8000,a0
	bcc.b	1f
	* banked ROM, check which bank
	lea		bank1_4000-0x4000,a1
	tst.b	current_bank
	bne.b	0f
	lea		bank0_4000-0x4000,a1
0:
	add.l	a1,a0
	POP_SR
	rts
1:
	* above 0x8000: ROM
	add.l	a6,a0
	POP_SR
	rts

address_1000_20FF_loaded_message:
	.asciz	"video address 0x1000-0x20FF loaded!"
address_2100_23FF_loaded_message:
	.asciz	"video address 0x2100-0x23FF loaded!"
address_2600_3FFF_loaded_message:
	.asciz	"video address 0x2600-0x3FFF loaded!"
	.align	2
	
switch_to_bank_00:
	clr.b	current_bank
	rts
	
switch_to_bank_01:
	move.b	#1,current_bank
	rts
	
* write color or video ram depending on address
* reminder: writing videoram doesn't necessarily has to refresh tile
* whereas writing to colorram refreshes tile

write_video_ram:
	move.w	a0,d4
	btst	#0,d4
	beq.b	0f
	jra	osd_w_colorram                    	| [$bc39: sta ($ad), y]	
0:
	jra	osd_w_videoram                    	| [$bc39: sta ($ad), y]	


write_to_pokey_00ce_y:
	* call this macro so no destination address check on A0
	GET_SCREEN_ADDRESS_Y	pokey_pointer_00ce                     	| [$4b3d: sta ($ce), y]
	sub.l	#0x2800,a0		| sub pokey base
	jra		osd_write_audio_pokey
	
write_to_pokey_00ce_x:
	* call this macro so no destination address check on A0

	lea	game_ram+pokey_pointer_00ce,a0
	READ_LE_WORD	a0
