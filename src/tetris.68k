	GET_ADDRESS	sound_event_counter_0c11                     	| [$414d: sty	$0c11]
	move.b	d2,(a0)                         	| [$414d: sty	$0c11]
	GET_ADDRESS	sound_event_counter_prev_0c10                     	| [$4150: cpy	$0c10]
	cmp.b	(a0),d2                          	| [$4150: cpy	$0c10]
	bne	l_4159                             	| [$4153: bne	$4159]
0:
	st.b	d0                                	| [$4155: lda #$ff]
	POP_SR                                 	| [$4157: plp]
	CLR_I_FLAG
	rts                                    	| [$4158: rts]
l_4159:
	lea 	extra_sound_event_table(pc),a0
	move.b	d0,d4
	add.b	d0,d0
	add.b	d0,d0
	move.l	(a0,d0.w),a0
	jsr		(a0)
	tst.b	d0
	bne.b	1f
	
