* original source code from PokeyNoise-SAP
* by Achim Haertel AKA Harry
* adapted by JOTD to just evaluate contents of pokey(s)
* and play
*
* only pokey emulation & sound replay was kept,
* 6502 code and exe load was removed

	.include	"custom.i"
	.global		evaluate_pokey_registers
	.global		update_sound
*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*here sound evaluation from 8-bit to amiga

* < A5: pokey registers

evaluate_pokey_registers:
*in a4 channeladress
	MOVE.L	A5,A4

1:
	MOVEQ.L	#0,D7
	MOVE.B	8(A5),D0
*test for 15 khz base frequency
	BTST	#0,D0
	BEQ.S	2f
	MOVE.B	#0x0,D7
	BRA.S	3f

*32 khz base frequency
2:	MOVE.B	#0x08,D7
*test for 1.7 mhz base frequency
3:	BTST	#6,D0
	BEQ.S	5f
*test for joined channel
	BTST	#4,D0
	BNE.S	4f
*channel alone in 1.79 mhz
	MOVE.B	#0x18,D7
	BRA.S	5f

*channel joined - 1.73 mhz
4:	MOVE.B	#0x10,D7
*test for 9bit-poly counter
5:	BTST	#7,D0
	BEQ.S	6f
*9-bit poly counter instead of 17 bit
	OR.B	#20,D7
*channel 1 and 2 mixed?
6:	BTST	#2,D0
	BEQ.S	7f
*yes...
	OR.B	#1,D7
7:
	BSR.W	EVALCH
	LEA.L	CH0SMPL(PC),A3
	BSR.W	SOUNDCHANGETEST
*	MOVE.L	D0,CH0SMPL
*	MOVE.W	D1,CH0LEN
*	MOVE.W	D2,CH0PER
*	MOVE.B	D3,CH0INT
*	MOVE.W	D6,CH0VOL
*	MOVE.B	D5,CH0CHANGED
	LEA.L	2(A4),A4
8:

9:	MOVEQ.L	#0,D7
	MOVE.B	8(A5),D0
*test for 15 khz base frequency
	BTST	#0,D0
	BEQ.S	10f
*15 khz base frequency
	MOVE.B	#0x0,D7
	BRA.S	11f

*32 khz base frequency
10:	MOVE.B	#0x08,D7
*test for 1.7 mhz base frequency (?????????)
11:	BTST	#6,D0
	BEQ.S	12f
	MOVE.B	#0x10,D7
*test for joined channel
12:	BTST	#4,D0
	BEQ.S	13f
	OR.B	#4,D7
*test for 9-bit poly counter
13:	BTST	#7,D0
	BEQ.S	14f
	OR.B	#20,D7
*channel  1 and 2 mixed?
14:	BTST	#2,D0
	BEQ.S	15f
	OR.B	#2,D7
15:
	BSR.W	EVALCH
	LEA.L	CH1SMPL(PC),A3
	BSR.W	SOUNDCHANGETEST
	LEA.L	2(A4),A4
16:


*channel c and d like a and b
17:	MOVEQ.L	#0,D7
	MOVE.B	8(A5),D0
	BTST	#0,D0
	BEQ.S	18f
	MOVE.B	#0x0,D7
	BRA.S	19f

18:	MOVE.B	#0x08,D7
19:	BTST	#5,D0
	BEQ.S	21f
	BTST	#3,D0
	BNE.S	20f
	MOVE.B	#0x18,D7
	BRA.S	21f

20:	MOVE.B	#0x10,D7
21:	BTST	#7,D0
	BEQ.S	22f
	OR.B	#20,D7
22:	BTST	#2,D0
	BEQ.S	23f
	OR.B	#1,D7
23:
	BSR.W	EVALCH
	LEA.L	CH2SMPL(PC),A3
	BSR.W	SOUNDCHANGETEST
	LEA.L	2(A4),A4
24:

25:	MOVEQ.L	#0,D7
	MOVE.B	8(A5),D0
	BTST	#0,D0
	BEQ.S	26f
	MOVE.B	#0x0,D7
	BRA.S	27f

26:	MOVE.B	#0x08,D7
27:	BTST	#5,D0
	BEQ.S	28f
	MOVE.B	#0x10,D7
28:	BTST	#3,D0
	BEQ.S	29f
	OR.B	#0x4,D7

29:	BTST	#7,D0
	BEQ.S	30f
	OR.B	#20,D7
30:	BTST	#1,D0
	BEQ.S	31f
	OR.B	#2,D7
31:
	BSR.W	EVALCH
	LEA.L	CH3SMPL(PC),A3
	BSR.S	SOUNDCHANGETEST
32:

	MOVE.B	8(A5),SPECIALSTORE
	RTS

SOUNDCHANGETEST:
*A3 IS POINTER ON CHxSMPL
*D3 TRUE, IF THE SAMPLE CAN BE PLAYED ONE TIME IN 0.018 SEC
*.2	TST.W	D6
*	BNE.S	.1
*	MOVE.L	#SILENCE,D0
*	MOVE.L	#4,D1
*	MOVE.L	#0x100,D2
1:
	CMP.L	(A3),D0
	BNE.S	2f
	SF.B	CH0NEWSAMPLE-CH0SMPL(A3)
	BRA.S	3f

2:	ST.B	CH0NEWSAMPLE-CH0SMPL(A3)
	ST.B	CH0CHANGED-CH0SMPL(A3)
	MOVE.L	D0,(A3)
	MOVE.W	D1,CH0LEN-CH0SMPL(A3)
3:
	CMP.W	CH0PER-CH0SMPL(A3),D2
	BEQ.S	4f
	MOVE.W	D2,CH0PER-CH0SMPL(A3)
	ST.B	CH0CHANGED-CH0SMPL(A3)
4:
	MOVE.W	D6,CH0VOL-CH0SMPL(A3)
	RTS

*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

EVALCH:
*A4 GIVES CHANNEL ADRESS
*	OFFSET 0 IS AUDFx
*	OFFSET 1 IS AUDCx
*D7 GIVES SPECIALBITS
*	#5 - 9-BIT-POLY COUNTER
*	#4,#3	CHANNEL "FREQUENCY"
*		0,0 - 15 KHZ
*		0,1 - 32 KHZ
*		1,0 - 1.73 MHZ (2 JOINED)
*		1,1 - 1.79 MHZ
*	#2 - CHANNEL IS JOINED
*	#1 - CHANNEL IS MODULATOR 
*	#0 - CHANNEL IS BEING MODULATED
*	     (NOTE 28.8.2000: EVENTUALLY, MODULATION WORKS AS FOLLOWING:
*		IMAGINE A WAVEFORM WILL BE PLAYED ON BOTH CHANNELS. NOW
*		IF BOTH CHANNELS HAVE POSITIVE OR BOTH NEGATIVE VALUE,
*		THE RESULTING OUTPUT WILL BE POSITIVE, IF ONE 'PLAYS'
*		POSITIVE AND ONE NEGATIVE VALUE, THE OUTPUT IS NEGATIVE)
*INTERNAL:
*D2 WILL BE (AUDFx+1) TIMES 0x100, REFERRED ON THE VALUE WHICH AUDFx AT
*	31960 HZ BASE FREQUENCY WOULD NEED TO HAVE, MAY ASSUME 
*	VALUES UP TO 0x7FFFF
*D6 WILL BE VOLUME
*
*RETURNED VALUES:
*D0 SAMPLE ADRESS
*D1 SAMPLE LENGTH
*D2 PERIOD
*D5 TRUE: CHANNEL CHANGED
*D6 VOLUME
*
*INTERNAL: (AB)USES 0xD240-0xD279(atari) for sound checks, that shouldnt
*		cause problems with correctly programmed sounds


*volume-only active?
	BTST	#4,1(A4)
	BNE.S	1f
*determine waveform (bit 5-7) on base of the poly counter
	MOVEQ.L	#0,D0
	MOVE.B	1(A4),D0
	MOVE.L	D0,D6
	AND.L	#0xF,D6
	LSR.B	#5,D0
	LSL.B	#2,D0
	JMP	2f(PC,D0.W)
1:	
	MOVEQ.L	#0,D6
	MOVE.L	#SILENCE,D0
	MOVE.L	#4,D1
	MOVE.L	#0x100,D2
	ST	D3
	RTS

2:	BRA.W	POLY0517
	BRA.W	POLY05
	BRA.W	POLY0405
	BRA.W	POLY05
	BRA.W	POLY17
	BRA.W	NOPOLY
	BRA.W	POLY04
	BRA.W	NOPOLY

POLY0517:	
	MOVEQ.L	#0,D3
*test for joined channel
	BTST	#2,D7
	BEQ.S	1f
*if joined, put AUDF of previous channel in bit 8-15,
*  AUDF of this channel in bit 16-23, rest of the bits are 0
	MOVEP.W	-2(A4),D3
	CLR.B	D3
	SWAP	D3
	MOVE.B	(A4),D3
	SWAP	D3
	BRA.S	2f

*normally AUDF in bit 8-15, rest of the bits are 0
1:	MOVEP.W	0(A4),D3
	CLR.B	D3

*test for channel frequency
2:	MOVE.B	D7,D1
	AND.W	#0x18,D1
	LSR.B	#1,D1
	JMP	3f(PC,D1.W)
3:	BRA.W	4f
	BRA.W	5f
	BRA.W	6f
	BRA.W	7f

*conversion of channel "frequency" to normperiod, result is in d2
*for 15 khz multiply with 114, then add 114*0x100 and divide by 28
4:	MOVEQ.L	#114,D4
	BSR.W	MULTIPLY
	MOVE.L	D2,D3
	ADD.L	#114*0x100,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE
	BRA.S	8f

5:	ADD.L	#0x100,D3
	MOVE.L	D3,D2
	BRA.S	8f

*for 1730 khz, add 7 and divide by 28
6:
	ADDQ.L	#7,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE
	BRA.S	8f

*for 1790 khz, add 4 and divide by 28
7:
	ADDQ.L	#4,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE

8:	MOVE.L	D5,D1
	MOVE.L	#0xFF,D5
	CMP.B	#0,0x60(A4)
	BNE.S	9f
	CMP.W	0x40(A4),D2
	BNE.S	9f
	MOVE.B	8(A5),D0
	CMP.B	SPECIALSTORE(PC),D0
	BNE.S	10f
	CMP.W	0x50(A4),D6
	BNE.S	10f
	MOVEQ.L	#0,D5
	BRA.S	10f

*for 'strange' waveforms, which play with some probability at the change
*  one sound and sometimes another, mark the change
9:	OR.L	#0xFF0000,D5
10:	MOVE.W	D2,0x40(A4)
	MOVE.W	D6,0x50(A4)
	MOVE.B	#0,0x60(A4)

*9-bit-poly-counter?
	BTST.L	#5,D7
	BNE.S	13f

*17-bit-poly counter aktive
*with period 137 the sampling was sampled on the amiga
	MOVE.L	#137,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
*lower than ffff isnt possible (produces only crack-sounds anyway)
	CMP.L	#0x10000,D2
	BLO.S	11f
	MOVE.L	#0xFFFF,D2
*lower than period 124 also isnt possible (in PAL), but maybe that should
*  simply become silenced?!?
11:	CMP.L	#124,D2
	BHS.S	12f
	MOVE.L	#124,D2
12:
*correct the volume
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0517_0,D0
	MOVE.W	#(POLY0517_0END-POLY0517_0)/2,D1
	RTS

*9-bit-poly counter active, means this is 0509-poly
13:	MOVE.L	D2,A0
	MOVE.L	D1,A1
	MOVE.L	A1,D3
	MOVEQ.L	#73,D4
	BSR.W	DIVIDE
	MOVE.L	A0,D2
	TST.W	D1
	BEQ.W	29f			|if playing value can be divided 
					*by 73, play waveform 0509D

	MOVE.L	A1,D3
	MOVEQ.L	#31,D4
	BSR.W	DIVIDE
	MOVE.L	A0,D2
	TST.W	D1
	BEQ.W	24f			|if playing value can be divided
					*by 31, play waveform 0509C

	MOVE.L	A1,D3
	MOVEQ.L	#7,D4
	BSR.W	DIVIDE
	MOVE.L	A0,D2
	TST.W	D1
	BEQ.W	18f			|if playing value can be divided
					*by 7, play waveform 0509B

14:					|.else play waveform 0517, which
	MOVE.L	#137,D4			|sounds equally to 0509A
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	15f
	MOVE.L	#0xFFFF,D2
15:	CMP.L	#124,D2
	BHS.S	16f
	MOVE.L	#124,D2
16:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0517_0,D0
	MOVE.W	#(POLY0517_0END-POLY0517_0)/2,D1
	RTS

17:	
	MOVEQ.L	#0,D6
	MOVE.L	#SILENCE,D0
	MOVE.L	#4,D1
	MOVE.L	#0x100,D2
	RTS

*if waveform changed, then silence channel with 1/3 probability 
18:	MOVE.L	D5,D0
	SWAP	D0
	TST.B	D0
	BEQ.S	19f
	BSR.W	RANDOM
	CMP.B	#0x50,D1
	BLO.W	17b
19:
20:	CMP.L	#0x280,D2
	BHI.S	22f
	MOVE.L	#178,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.W	#124,D2
	BHS.S	21f
	MOVE.L	#124,D2
21:	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0509B_0,D0
	MOVE.W	#(POLY0509B_0END-POLY0509B_0)/2,D1
	RTS

22:	ADD.L	D2,D2
	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	23f
	MOVE.L	#0xFFFF,D2
23:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0509B_1_5,D0
	MOVE.W	#(POLY0509B_1_5END-POLY0509B_1_5)/2,D1
	RTS

24:
	MOVE.L	D5,D0
	SWAP	D0
	TST.B	D0
	BEQ.S	25f
	BSR.W	RANDOM
	CMP.B	#0x50,D1
	BLO.W	17b
25:
26:	ADD.L	D2,D2
	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	27f
	MOVE.L	#0xFFFF,D2
27:	CMP.L	#124,D2
	BHS.S	28f
	MOVE.L	#124,D2
28:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0509C_1_5,D0
	MOVE.W	#(POLY0509C_1_5END-POLY0509C_1_5)/2,D1
	RTS

29:
30:	ADD.L	D2,D2
	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	31f
	MOVE.L	#0xFFFF,D2
31:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY0509D_1_5,D0
	MOVE.W	#(POLY0509D_1_5END-POLY0509D_1_5)/2,D1
	RTS

POLY17:	
	MOVEQ.L	#0,D3
	BTST	#2,D7
	BEQ.S	1f
	MOVEP.W	-2(A4),D3
	CLR.B	D3
	SWAP	D3
	MOVE.B	(A4),D3
	SWAP	D3
	BRA.S	2f

1:	MOVEP.W	0(A4),D3
	CLR.B	D3
2:	MOVE.B	D7,D1
	AND.W	#0x18,D1
	LSR.B	#1,D1
	JMP	3f(PC,D1.W)
3:	BRA.W	4f
	BRA.W	5f
	BRA.W	6f
	BRA.W	7f

4:	MOVEQ.L	#114,D4
	BSR.W	MULTIPLY
	MOVE.L	D2,D3
	ADD.L	#114*0x100,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE
	BRA.S	8f

5:	ADD.L	#0x100,D3
	MOVE.L	D3,D2
	BRA.S	8f

6:
	ADDQ.L	#7,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE
	BRA.S	8f

7:
	ADDQ.L	#4,D3
	MOVEQ.L	#28,D4
	BSR.W	DIVIDE

8:	MOVE.L	D5,D1
	MOVE.L	#0xFF,D5
	CMP.B	#4,0x60(A4)
	BNE.S	9f
	CMP.W	0x40(A4),D2
	BNE.S	9f
	MOVE.B	8(A5),D0
	CMP.B	SPECIALSTORE(PC),D0
	BNE.S	10f
	CMP.W	0x50(A4),D6
	BNE.S	10f
	MOVEQ.L	#0,D5
	BRA.S	10f

9:	OR.L	#0xFF0000,D5
10:	MOVE.W	D2,0x40(A4)
	MOVE.W	D6,0x50(A4)
	MOVE.B	#4,0x60(A4)

	BTST.L	#5,D7
	BNE.S	14f

	TST.B	AUDIOCORRECTRH
	BEQ.S	11f
	CMP.L	#0x2F0,D2
	BHS.S	11f
	MOVEQ.L	#0,D6
11:

	MOVE.L	#132,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	12f
	MOVE.L	#0xFFFF,D2
12:	CMP.L	#124,D2
	BHS.S	13f
	MOVE.L	#124,D2
13:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY17_0,D0
	MOVE.W	#(POLY17_0END-POLY17_0)/2,D1
	RTS

14:	MOVE.L	D2,A0
	MOVE.L	D1,A1
	MOVE.L	A1,D3
	MOVEQ.L	#73,D4
	BSR.W	DIVIDE
	MOVE.L	A0,D2
	TST.W	D1
	BEQ.W	27f

	MOVE.L	A1,D3
	MOVEQ.L	#7,D4
	BSR.W	DIVIDE
	MOVE.L	A0,D2
	TST.W	D1
	BEQ.W	20f

15:
	CMP.L	#0x280,D2
	BHS.S	17f
	MOVE.L	#178,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#124,D2
	BHS.S	16f
	MOVE.L	#124,D2
16:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY09A_0,D0
	MOVE.W	#(POLY09A_0END-POLY09A_0)/2,D1
	RTS

17:
	ADD.L	D2,D2
	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	18f
	MOVE.L	#0xFFFF,D2
18:
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY09A_1_5,D0
	MOVE.W	#(POLY09A_1_5END-POLY09A_1_5)/2,D1
	RTS

19:	
	MOVEQ.L	#0,D6
	MOVE.L	#SILENCE,D0
	MOVE.L	#4,D1
	MOVE.L	#0x100,D2
	RTS

20:	MOVE.L	D5,D0
	SWAP	D0
	TST.B	D0
	BEQ.S	21f
	BSR.W	RANDOM
	CMP.B	#0x50,D1
	BLO.W	19b
21:
22:	CMP.L	#0x280,D2
	BHI.S	24f
	MOVE.L	#178,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.W	#124,D2
	BHS.S	23f
	MOVE.L	#124,D2
23:	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY09B_0,D0
	MOVE.W	#(POLY09B_0END-POLY09B_0)/2,D1
	RTS

24:	CMP.L	#0x500,D2
	BHI.S	25f
	ADD.L	D2,D2
	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	MULU.W	#0x445,D6
	LSR.W	#8,D6
	MOVE.L	#POLY09B_1_5,D0
	MOVE.W	#(POLY09B_1_5END-POLY09B_1_5)/2,D1
	RTS

25:	MOVE.L	D2,D3
	MOVE.L	#5,D4
	BSR.W	DIVIDE
	MOVE.L	#140,D4
	MOVE.L	D2,D3
	BSR.W	MULTIPLY
	LSR.L	#8,D2
	CMP.L	#0x10000,D2
	BLO.S	26f
