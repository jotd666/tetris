*DIVIDES LONGWORD/WORD IN LONGWORD
*D3/D4 IN D2, REST IN D1

DIVIDE:	
	MOVE.L	D3,D1
	CLR.W	D1
	SWAP	D1
	DIVU.W	D4,D1
	MOVE.W	D1,D2
	SWAP	D2
	MOVE.W	D3,D1
	DIVU.W	D4,D1
	MOVE.W	D1,D2
	CLR.W	D1
	SWAP	D1
	MOVEQ.L	#0,D0
	RTS


*MULTIPLIES LONGWORD*WORD IN LONGWORD
*D3*D4 IN D2

MULTIPLY:
	MOVE.L	D3,D1
	CLR.W	D1
	SWAP	D1
	MULU.W	D4,D1
	CMP.L	#0x10000,D1
	BHS.S	1f
	SWAP	D1
	MOVEQ.L	#0,D2
	MOVE.W	D3,D2
	MULU.W	D4,D2
	ADD.L	D1,D2
	BCS.S	1f
	MOVEQ.L	#0,D0
	RTS

